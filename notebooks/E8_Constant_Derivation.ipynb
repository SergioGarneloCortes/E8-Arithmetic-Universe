# Jupyter Notebook para verificación interactiva

import numpy as np
import matplotlib.pyplot as plt
from src.e8_physics import E8ArithmeticUniverse
from src.modular_forms import E8ModularForms

# Inicializar el universo E8
universe = E8ArithmeticUniverse()
modular = E8ModularForms()

print("=== E8 ARITHMETIC UNIVERSE VERIFICATION ===")
print(f"E8 Golden Ratio: {universe.phi_e8}")
print(f"Precision: {universe.precision} decimal places")

# 1. Verificar constante cosmológica
print("\n1. COSMOLOGICAL CONSTANT")
lambda_result = universe.compute_cosmological_constant()
print(f"Λ = {lambda_result['value']:.4e} ± {lambda_result['uncertainty']:.1e} m⁻²")
print(f"Experimental: ~1.10e-52 m⁻²")

# 2. Verificar masas de fermiones
print("\n2. FERMION MASSES")
masses = universe.compute_fermion_masses()
for particle, mass in masses.items():
    unit = "GeV" if mass > 1 else "MeV"
    print(f"{particle:8}: {mass:.6f} {unit}")

# 3. Verificar acoplamientos de gauge
print("\n3. GAUGE COUPLINGS")
couplings = universe.compute_gauge_couplings()
for coupling, value in couplings.items():
    print(f"{coupling:15}: {value:.6f}")

# 4. Verificar materia oscura
print("\n4. DARK MATTER")
dm = universe.compute_dark_matter_mass()
print(f"m_χ = {dm['value']:.2f} ± {dm['uncertainty']:.1f} {dm['units']}")

# 5. Verificar invariancia modular
print("\n5. MODULAR INVARIANCE VERIFICATION")
tau = 1j  # Punto en el semiplano superior
invariance = modular.modular_invariance_check(tau)
print(f"Relative error: {invariance['relative_error']:.2e}")

# Gráfico de comparación con datos experimentales
experimental_masses = {
    'top': 172.76, 'bottom': 4.18, 'charm': 1.27,
    'tau': 1.77693, 'muon': 0.105658, 'electron': 0.000510999
}

particles = list(masses.keys())
theoretical = [masses[p] for p in particles]
experimental = [experimental_masses[p] for p in particles]

plt.figure(figsize=(10, 6))
x_pos = np.arange(len(particles))
width = 0.35

plt.bar(x_pos - width/2, theoretical, width, label='E8 Prediction', alpha=0.7)
plt.bar(x_pos + width/2, experimental, width, label='Experimental', alpha=0.7)

plt.xlabel('Particles')
plt.ylabel('Mass (GeV)')
plt.title('E8 Prediction vs Experimental Fermion Masses')
plt.xticks(x_pos, particles)
plt.yscale('log')
plt.legend()
plt.grid(True, alpha=0.3)
plt.tight_layout()
plt.show()